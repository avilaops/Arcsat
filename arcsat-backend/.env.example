# Configuração do servidor
SERVER_HOST=0.0.0.0
SERVER_PORT=3000
SERVER_WORKERS=4

# Database PostgreSQL
DATABASE_URL=postgresql://arcsat:arcsat@localhost:5432/arcsat

# Redis (para filas)
